{
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "examples/*.html", "use": "@vercel/static" }
    ],
    "routes": [
      
      {
        "src": "/examples/(.*)",
        "dest": "/examples/$1"
      },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  